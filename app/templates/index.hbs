{{!-- <ProjectInfo @project={{@model.project}}/> --}}
<main class="main">
  <div class="header">
    <div class="group-col">
      <h1>Dashboard</h1>
      <p class="header-sub">Hello there, enjoy your coding! <span class="header-emoji">ðŸ‘‹</span></p>
    </div>

    <div class="">
      <button type="button" class="rounded-full bg-gray px-10 py-2">Costumize</button>
    </div>
  </div>

  <div class="main-inner">
    <div class="card">
      <div class="card-header">
        <div class="group-row group-center">
          <svg class="card-icon" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1" d="M19 20H5a2 2 0 01-2-2V6a2 2 0 012-2h10a2 2 0 012 2v1m2 13a2 2 0 01-2-2V7m2 13a2 2 0 002-2V9a2 2 0 00-2-2h-2m-4-3H9M7 16h6M7 8h6v4H7V8z"></path>
          </svg>
          <div class="card-title">
            Ember Blog Feed
          </div>
        </div>
          <a href="https://blog.emberjs.com/">
            <svg class="card-icon" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"></path>
            </svg>
          </a>
        </div>
      <div class="card-content">
          {{#async-await this.articles as |list|}}
        <div class="blog">

            {{#each list as |article|}}
              <div class="blog-article">
                <div class="article-title">
                  {{article.elements.[0].elements.[0].text}}
                </div>
                <div class="article-date">
                  {{format-datetime article.elements.[3].elements.[0].text}}
                </div>
                <div class="article-summary">
                  {{{article.elements.[6].elements.[0].text}}}
                </div>
              </div>
            {{/each}}
        </div>
            {{else}}
            <div class="">spin </div> 
          {{/async-await}}
      </div>
    </div>
  </div>
</main>

